    border-color: rgba(0, 0, 0, 0);
    background-color: rgba(0, 0, 0, 0);
}

.button:hover {
    border-radius: 20px;
    scale: 1.1 1.1;
    border-color: rgba(0, 0, 0, 0);
}

#SettingsContainer {
    flex-direction: row;
    padding: 20px;
    justify-content: space-between;
    align-self: auto;
    align-content: flex-start;
    align-items: stretch;
    opacity: 0;
    flex-grow: 1;
    background-color: rgba(145, 143, 154, 0.16);
    background-image: url("project://database/Assets/Imports/2023/2910BackgroundNoEffects.png?fileID=21300000&guid=7c7e2fb113e139a49a95ce1068881859&type=3#2910BackgroundNoEffects");
}

.back {
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    align-self: flex-start;
    padding: 0 0 0 10px;
}

#SettingsLabel {
    font-size: 48px;
    color: rgb(204, 204, 226);
    rotate: 0;
}

#BackButton {
    margin-right: 20px;
    scale: 2 2.5;
    background-image: url("project://database/Assets/Imports/Icons/arrow_back_48dp_E8EAED_FILL0_wght400_GRAD0_opsz48.png?fileID=21300000&guid=e1078f2c2f8f8ce4c8cbab2549268b79&type=3#arrow_back_48dp_E8EAED_FILL0_wght400_GRAD0_opsz48");
    background-color: rgba(0, 0, 0, 0);
    -unity-background-image-tint-color: rgb(204, 204, 226);
    border-color: rgba(0, 0, 0, 0);
    transition-duration: 0.05s;
}

#BackButton:hover {
    background-color: rgb(22, 24, 49);
    scale: 1.75 2;
    -unity-background-image-tint-color: rgb(204, 204, 226);
    border-radius: 5px;
    border-top-width: 3px;
    border-right-width: 3px;
    border-bottom-width: 3px;
    border-left-width: 3px;
}

.unity-tab__header {
    background-color: rgba(229, 223, 223, 0);
