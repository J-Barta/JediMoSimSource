}

.button {
    padding: 15px;
    margin: 10px;
    color: rgb(204, 204, 226);
    font-size: 48px;
    -unity-font-definition: url("project://database/Assets/Fonts/ARIALNI.TTF?fileID=12800000&guid=19bd3826c53d0ae48a93ad832a627100&type=3#ARIALNI");
